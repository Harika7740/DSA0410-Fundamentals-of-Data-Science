import statistics

# Sample dataset
data = [10, 20, 20, 30, 40, 50]

# Basic statistical methods
mean = statistics.mean(data)
median = statistics.median(data)
mode = statistics.mode(data)
std_dev = statistics.stdev(data)

# Print results
print("Mean:", mean)
print("Median:", median)
print("Mode:", mode)
print("Standard Deviation:", std_dev)
